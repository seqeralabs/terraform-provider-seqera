resource "seqera_data_studios" "my_datastudios" {
  auto_start            = true
  compute_env_id        = resource.seqera_compute_env.aws_batch_compute_env.compute_env_id
  conda_environment     = ""
  cpu                   = 2
  data_studio_tool_url  = "public.cr.seqera.io/platform/data-studio-jupyter:4.2.5-0.7"
  description           = "Data studio generated by Terraform"
  gpu                   = 0
  initial_checkpoint_id = 0
  is_private            = true
  lifespan_hours        = 9
  memory                = 5
  mount_data            = []
  name                  = "Terraform-Data-Studio"
  spot                  = true
  workspace_id          = resource.seqera_workspace.my_workspace.id
  session_id            = "terraform-test"
}